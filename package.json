    "start": "npm run prod:server",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "nuxt": "^2.0.0",
    "ts-node": "^8.4.1"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.2.9",
    "@types/express": "^4.17.1",
    "nodemon": "^1.18.9"
  }
}
