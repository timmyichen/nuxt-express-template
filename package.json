{
  "name": "temp",
  "version": "1.0.0",
  "description": "My pioneering Nuxt.js project",
  "author": "timmyichen",
  "private": true,
  "scripts": {
    "prod:server": "NODE_ENV=production ts-node --files  --transpile-only ./server/index.ts",
    "dev:server": "ts-node --files --transpile-only ./server/index.ts",
    "dev": "nodemon --watch 'server/**/*.ts' --exec 'npm run dev:server'",
